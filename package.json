{
  "name": "wambi-connect",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tsc": "tsc",
    "export": "next export",
    "test": "jest --no-cache --forceExit --coverage",
    "test:sonar": "jest --no-cache --forceExit --coverage --testResultsProcessor=jest-sonar-reporter",
    "prepare": "husky install",
    "release:version": "version-select",
    "release:tag": "sh ./support/versioning/versioning.sh",
    "release:create": "npm run release:version && yarn release:tag",
    "commit": "npx git-cz"
  },
  "dependencies": {
    "@types/node": "^20.4.6",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "axios": "^1.3.4",
    "body-scroll-lock": "^4.0.0-beta.0",
    "eslint": "^8.46.0",
    "eslint-config-next": "^13.4.12",
    "next": "^13.4.12",
    "next-translate-routes": "^1.10.2",
    "react": "^18.2.0",
    "react-animated-numbers": "^0.16.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.4.4",
    "react-reveal": "^1.2.2",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 2
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/chance": "^1.1.3",
    "@types/react-modal-video": "^1.2.1",
    "@types/react-slick": "^0.23.10",
    "@types/styled-components": "^5.1.26",
    "chance": "^1.1.11",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "lint-staged": "^13.2.3",
    "styled-components": "^6.0.7",
    "ts-jest": "^29.1.1",
    "version-select": "^1.0.13"
  }
}
