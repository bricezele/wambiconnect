    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "^6.3.2",
    "ts-node": "^10.9.1",
    "typescript": "5.0.3"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 2
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/chance": "^1.1.3",
